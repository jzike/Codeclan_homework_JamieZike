library(tidyverse)
library(janitor)
library(modelr)
project <- read_csv("data/project_management.csv") %>% clean_names()
project %>%
ggplot(aes(x = estimated_length,
y = actual_length)) +
geom_point()
project %>%
summarise(cor(estimated_length, actual_length))
library(tidyverse)
library(janitor)
library(modelr)
project <- read_csv("data/project_management.csv") %>% clean_names()
project %>%
ggplot(aes(x = estimated_length,
y = actual_length)) +
geom_point()
project %>%
summarise(cor(estimated_length, actual_length))
model <- lm(formula = actual_length ~ estimated_length,
data = project)
model
View(project)
project %>%
add_predictions(model) %>%
add_residuals(model)
project %>%
ggplot(aes(x = estimated_length)) +
geom_point(aes(y = actual_length)) +
geom_line(aes(y = pred), col = "red")
project <- project %>%
add_predictions(model) %>%
add_residuals(model)
project %>%
ggplot(aes(x = estimated_length)) +
geom_point(aes(y = actual_length)) +
geom_line(aes(y = pred), col = "red")
